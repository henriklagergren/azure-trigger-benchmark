{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request",
        "minSamplingPercentage": 100.0
      }
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:00.100",
      "batchSize": 1,
      "visibilityTimeout": "00:00:00"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}