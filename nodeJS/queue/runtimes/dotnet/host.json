{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:00.100",
            "batchSize": 32,
            "newBatchThreshold": 16,
            "visibilityTimeout": "00:00:00",
            "maxDequeueCount": 5
        }
    }
}