  "files": [
    "workloads/workload.ts",
    "http/index.ts",
    "shared/index.ts",
    "infra/index.ts",
    "storage/index.ts",
    "nodeJS/queue/index.ts",
    "database/index.ts",
    "service_Bus/index.ts",
    "timer/index.ts",
    "event_hub/index.ts"
  ]
}
